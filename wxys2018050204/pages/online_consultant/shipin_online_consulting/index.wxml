  </view>
  <scroll-view scroll-y="true" class="bottom-3-2">
    <view class="bottom-3-2-1" wx:for="{{yishengpaiban}}" wx:key="PBMXXH" bindtap='ShowViewPBHX' data-PBMXXH="{{item.PBMXXH}}" data-PBRQ="{{item.PBRQ}}" data-PBRQ_y="{{item.PBRQ_y}}" data-WEEKDAY="{{item.WEEKDAY}}" data-PBKSSJ="{{item.PBKSSJ}}" data-PBJSSJ="{{item.PBJSSJ}}">
      {{item.PBRQ}}  {{item.WEEKDAY}}  {{item.PBKSSJ}}-{{item.PBJSSJ}}
    </view>
  </scroll-view>
  <view class="bottom-3-3" bindtap='HideViewPBRQ'>
    取消
  </view>
</view>


<view class="bottom-4" style="display:{{ViewPBHX}}">
  <view class="bottom-4-1">
