    <view class="bottom-4-2">      
      <navigator  class="bottom-4-2-1" wx:for="{{yishengpaibanhaoxu}}" wx:key="autoid" url="../shipin_online_consulting_pay/index?PBMXXH={{PBMXXH}}&PBRQ={{PBRQ}}&PBRQ_y={{PBRQ_y}}&WEEKDAY={{WEEKDAY}}&TIME={{item.TIME}}&HX={{item.HX}}">
        {{item.TIME}}
      </navigator>
    </view>
  </scroll-view>
  <view class="bottom-4-3" bindtap='HideViewPBHX'>
    取消
  </view>
</view>

