/* pages/favorite/note_search/index.wxss */
page{
  background: #f5f5f5;
}
.wx-search-ipt-groups{
  display: flex;
  background: #efeff4;
  padding: 0px 10px;
  align-items: center;
  height: 40px;
}
.search-ipt {
  height:25px;
  line-height:25px;
  border-radius: 5px;
  background: #fff;
  padding:0px 30px 0px 30px;
}
.search-ipt-ico{
  width:14px;
  height:14px;
  top:13px;
  left:18px;
}
.cancel-search{
  color:#6cd46d;
  font-size:15px;
  padding-left:10px;
}
.common-diseases {
  background: none;
}
.common-label-li{
  background:#fff;
  font-size:13px;
  padding:2px;
  border-radius:15px;
  margin:5px;
  padding:5px 8px;
  color:#818182;
  border:1px solid #efefef;
}
