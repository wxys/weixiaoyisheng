/*
  滑动删除
*/

view {
  box-sizing: border-box;
}

.items {
  width: 100%;

}

.item {
  position: relative;
  border-bottom: 1rpx solid #ebebeb;

  overflow: hidden;
}

.inner {
  position: absolute;
  top: 0;
}

.inner.txt {
  background-color: #fff;
  width: 100%;
  z-index: 5;
  transition: left 0.2s ease-in-out;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.inner.del, .inner.set-default {
  width: 70px;
  text-align: center;
  z-index: 4;
  color: #fff;
  font-size: 0.8rem;
  font-weight: 500;
  height: 75px;
  line-height: 75px;
}

.inner.del {
  background-color: #e64340;
  right: 0px;
}

.inner.set-default {
  background-color: #bababa;
  right: 70px;
}

.item-icon {
  width: 64rpx;
  vertical-align: middle;
  margin-right: 16rpx;
}

.thumb {
  width: 200px;
  height: 200px;
  -webkit-overflow-scrolling: touch;
  overflow: scroll;
}

