  align-items: center;
  width: 65px;
  justify-content: center;
  background: #fff;
  margin-right:5px;
  color:#ff8100;
}
.serach-select-region image {
  width: 19px;
  height: 19px;
  margin-left: 2px;
}
.search-ipt-ico {
  position: absolute;
  z-index: 999;
  width: 17px;
  top: 14px;
  left: 10px;
}
.clear-search-ico {
  position: absolute;
  z-index: 999;
  width: 20px;
  height: 20px;
  top: 12px;
  right: 10px;

}
.li-title {
  padding: 10px 10px 0px 10px;
  font-size: 0.8rem;
  color: #d1d1d1;
  display: flex;
}

.li-title-text {
  height: 20px;
  line-height: 20px;
  flex-grow: 1;
  font-size: 0.75rem;
}

.clear-search-reacord {
  width: 20px;
  height: 20px;
}

.user-search-record-list {
  padding: 5px 10px 0px 10px;
  display: table;
}

.serach-record {
  background: #f4f4f4;
  padding: 8px;
  border-radius: 15px;
  font-size: 12px;
  color: #000;
  margin: 5px;
  float: left;
}

.empty-search-reacord {
  color: #d1d1d1;
  text-align: center;
  font-size: 0.85rem;
  width: 710rpx;
  padding: 10px 0px;
}

.search-li {
  display: flex;
}

.search-li-text {
  color: #58595a;
  display: block;
  height: 40px;
  border-bottom: 1px dashed #ebebeb;
  font-size: 0.8rem;
  width: 710rpx;
