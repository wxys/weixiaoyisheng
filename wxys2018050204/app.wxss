/**app.wxss**/

page {
  background: #f8f8f8;
  font-family: -apple-system-font,
                 "Helvetica Neue",
                 "PingFang SC",
                 "Hiragino Sans GB",
                 "Microsoft YaHei",
                 sans-serif;
}
p{
  display: block;
}
.h1 {
  font-size: 25px;
}

.h2 {
  font-size: 20px;
}

.h3 {
  font-size: 18px;
}

.strong {
  font-weight: 600;
}
button::after{
  border:none;
}
/*
    弹窗提醒样式
*/

.remind {
  position: absolute;
  height: 45px;
  line-height: 45px;
  width: 200px;
  z-index: 9999;
  background: #2a292d;
  color: #fff;
  font-size: 0.8rem;
  border-radius: 9px;
  text-align: center;
  top: 50%;
  margin: -20px 0px 0px -100px;
  left: 50%;
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}

/*
  loading样式
*/

.loading-groups {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 120px;
  height: 120px;
  position: absolute;
  z-index: 999;
  top: 50%;
  left: 50%;
  flex-direction: column;
  /*border-radius: 30%;
  background: #fff;*/
  margin: -75px 0px 0px -60px;
}

.loading-ico {
  width: 100px;
  height: 100px;
  /*border-radius:15px;*/
}

.loading-text {
  margin-top: 10px;
  font-size: 0.85rem;
  color: #666;
  font-weight: 500;
}

/*
  角标样式
*/

.corner-mark {
  background: #d14331;
  width: 14px;
  height: 14px;
  border-radius: 50%;
  color: #fff;
  font-size: 10px;
  position: absolute;
  line-height: 14px;
  text-align: center;
  margin-top: 5px;
  margin-left: 3px;
}

/*
    栏目样式
*/

.column-title {
  display: flex;
  padding: 10px;
  margin-top: 8px;
  font-weight: 500;
  color: #000;
  font-size: 31rpx;
  line-height: 15px;
  align-items: center;
  background: #fff;
}

.column-title.no-margin {
  margin-top: 0px;
}

.column-text {
  /*
  border-left: 4px solid #59bdca;
  padding-left: 10px;
  */
  flex-grow: 1;
  display: flex;
  align-items: center;
}

.column-right-text {
  display: flex;
}

.column-remind {
  font-size: 26rpx;
  color: #666;
  font-weight: 400;
  margin: 0px 5px;
}

.column-ico {
  width: 16px;
  height: 16px;
  margin-left: 2px;
}

.es-list-li {
  padding: 20rpx;
  display: flex;
  flex-direction: row;
  background: #fff;
 
  align-items: center;
  justify-content: center;
}

.es-list-li .list-text {
  flex: 1.0;
  font-size: 0.8rem;
  color: #000;
}

.es-list-li .list-ico {
  width: 16px;
  height: 16px;
}

.es-list-li.flex-start {
  justify-content: flex-start;
}

.es-list-li.between {
  justify-content: space-between;
}

.es-list-li.flex-column {
  flex-direction: column;
}

.es-list-li.center {
  align-items: center;
}

.es-list-li.start {
  align-items: flex-start;
}

.es-list-li.no-border {
  border: 0;
}

.es-list-li.top-border {
  border-top: 1px solid #f3f3f3;
}

.es-list-li.relative {
  position: relative;
}

/*
  搜索框部分样式
*/

.search-ipt-groups {
  display: flex;
  background: #fff;
  padding:0px 5px;
  align-items: center;
  height: 45px;
  /*border-bottom: 1px solid #ebebeb;*/
}

.search-ipt {
  height: 35px;
  border: 0px;
  background: #f5f5f5;
  display: block;
  padding: 0px 30px 0px 35px;
  font-size: 0.85rem;
  line-height: 35px;
  color: #555;
  border-radius: 15px;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  -webkit-transition: border-color ease-in-out 0.15s, -webkit-box-shadow ease-in-out 0.15s;
  -o-transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
  transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
  flex: 1.0;
}
.serach-select-region {
  padding: 5px 0px;
  font-size: 0.8rem;
  /*margin-right:5px;*/
