}

.search-li-icon {
  width: 25px;
  height: 25px;
  margin-top: 7.5px;
  margin-right: 5px;
}

.empty-remind {
  padding: 10px;
  font-size: 0.85rem;
  text-align: center;
}

/*
  科室列表部分
*/

.departments-list {
